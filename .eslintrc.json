{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions": 0,
        "implicit-arrow-linebreak":0,
        "import/prefer-default-export": "off",
        "function-paren-newline":0,
        "comma-dangle": ["error", {

          "functions": "error"
      }]
      
    }
}


